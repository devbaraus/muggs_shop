{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "dev:remix": "netlify dev",
    "build:css": "tailwindcss -i ./styles/tailwind.css -o ./app/styles/tailwind.css --minify",
    "dev:css": "tailwindcss -i ./styles/tailwind.css -o ./app/styles/tailwind.css --watch",
    "dev": "dotenv -- concurrently \"npm run dev:css\" \"npm run dev:remix\"",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.5",
    "@netlify/functions": "^0.10.0",
    "@remix-run/netlify": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "date-fns": "^2.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.0.0",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.19",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
